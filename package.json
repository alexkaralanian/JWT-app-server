{
    "name": "server",
    "version": "0.1.0",
    "description": "express server and sequelize/postgres-db back-end",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "build": "NODE_ENV=production node server",
        "dev":
            "NODE_ENV=development concurrently \"npm run dev-server\" \"npm run client\"",
        "test": "NODE_ENV=test mocha",
        "dev-server": "nodemon server",
        "server": "babel-node server",
        "client": "node start-client.js",
        "jest": "jest-watch",
        "gitpush":
            "git add . && git commit -m 'new commit' && git push origin master"
    },
    "author": "Alex Karalanian",
    "devDependencies": {
        "babel-cli": "6.14.0",
        "babel-core": "^6.25.0",
        "babel-jest": "^19.0.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-2": "^6.22.0",
        "chai": "^4.1.1",
        "concurrently": "^3.5.0",
        "enzyme": "^2.9.1",
        "eslint": "^4.4.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-config-prettier": "^2.3.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jest": "^20.0.3",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-prettier": "^2.1.2",
        "eslint-plugin-react": "^7.2.0",
        "jest": "^20.0.4",
        "mocha": "^3.5.0",
        "nodemon": "^1.11.0",
        "prettier": "^1.6.1",
        "regenerator-runtime": "^0.10.5",
        "supertest": "^3.0.0"
    },
    "dependencies": {
        "axios": "^0.16.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.15.0",
        "cookie-session": "^1.3.0",
        "cors": "^2.8.4",
        "express": "^4.13.4",
        "express-jwt": "^5.3.0",
        "googleapis": "^20.1.0",
        "jsonwebtoken": "^7.4.1",
        "lodash": "^4.17.4",
        "md5": "^2.2.1",
        "morgan": "^1.7.0",
        "passport": "^0.3.2",
        "passport-facebook": "^2.1.1",
        "passport-google-oauth": "^1.0.0",
        "passport-jwt": "^3.0.0",
        "pg": "^6.1.5",
        "pg-hstore": "^2.3.2",
        "sequelize": "^3.30.4"
    },
    "jest": {
        "testEnvironment": "jest-environment-node"
